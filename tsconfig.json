{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "availability-calender": [
        "dist/availability-calender"
      ],
      "availability-calender/*": [
        "dist/availability-calender/*"
      ],
      "ngx-flatpickr-v2": [
        "dist/ngx-flatpickr-v2"
      ],
      "ngx-flatpickr-v2/*": [
        "dist/ngx-flatpickr-v2/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}